<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <!-- The identifier that must be unique within the hosting gallery -->
    <id>$id$</id>
    <title>Luis Client Library</title>
    <version>$version$</version>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <projectUrl>https://github.com/laazik/LuisClient</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <developmentDependency>false</developmentDependency>
    <description>Luis client library generated from Swagger and modified for consistency and size.</description>
    
    <!-- Dependencies are automatically installed when the package is installed -->
    <dependencies>
      <dependency id="NewtonSoft.Json" version="[10.0.3,11.0.0)" />
      <dependency id="RestSharp" version="[105.2.3,106.0.0)" />
    </dependencies>
  </metadata>
  
  <files>
    <!-- A readme.txt will be displayed when the package is installed -->
    <file src="..\..\README.md" target="" />
    <file src="..\..\docs\**\*.*" target="docs" />
  </files>
</package>